
shift + alt - m : /Users/glebchanskiy/.config/yabai/plugins/swap.sh 9 

alt - j : yabai -m window --focus south
alt - k : yabai -m window --focus north
alt - h : yabai -m window --focus west
alt - l : yabai -m window --focus east

shift + alt - j : yabai -m window --swap south
shift + alt - k : yabai -m window --swap north
shift + alt - h : yabai -m window --swap west
shift + alt - l : yabai -m window --swap east

ctrl + alt - j : yabai -m window --warp south
ctrl + alt - k : yabai -m window --warp north
ctrl + alt - h : yabai -m window --warp west
ctrl + alt - l : yabai -m window --warp east

shift + alt - r : yabai -m space --rotate 270
shift + alt - y : yabai -m space --mirror y-axis
shift + alt - x : yabai -m space --mirror x-axis
shift + alt - t : yabai -m window --toggle float --grid 4:4:1:1:2:2

shift + alt - m : yabai -m window --toogle zoom-fullscreen
shift + alt - e : yabai -m space --balance

shift + alt - p : yabai -m window --space prev
shift + alt - n : yabai -m window --space next

shift + alt - left : yabai -m window west --resize right:-20:0 2> /dev/null || yabai -m window --resize right:-20:0
shift + alt - down : yabai -m window north --resize bottom:0:20 2> /dev/null || yabai -m window --resize bottom:0:20
shift + alt - up : yabai -m window south --resize top:0:-20 2> /dev/null || yabai -m window --resize top:0:-20
shift + alt - right : yabai -m window east --resize left:20:0 2> /dev/null || yabai -m window --resize left:20:0

alt - q : yabai -m space --focus 1
alt - w : yabai -m space --focus 2
alt - e : yabai -m space --focus 3
alt - a : yabai -m space --focus 4
alt - s : yabai -m space --focus 5
alt - d : yabai -m space --focus 6
alt - b : yabai -m space --focus 7
alt - c : yabai -m space --focus 8
alt - z : yabai -m space --focus 9


shift + alt - q : yabai -m window --space 1 && yabai -m space --focus 1
shift + alt - w : yabai -m window --space 2 && yabai -m space --focus 2
shift + alt - e : yabai -m window --space 3 && yabai -m space --focus 3
shift + alt - a : yabai -m window --space 4 && yabai -m space --focus 4
shift + alt - s : yabai -m window --space 5 && yabai -m space --focus 5
shift + alt - d : yabai -m window --space 6 && yabai -m space --focus 6
shift + alt - b : yabai -m window --space 7 && yabai -m space --focus 7
shift + alt - c : yabai -m window --space 8 && yabai -m space --focus 8
shift + alt - z : yabai -m window --space 9 && yabai -m space --focus 9
